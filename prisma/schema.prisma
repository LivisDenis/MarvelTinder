generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model heroes {
  id       Int    @id @default(autoincrement())
  name     String @unique
  image    String @unique
  likes    Int    @default(0)
  dislikes Int    @default(0)
}
